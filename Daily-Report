            int hoursStudied = Console.WriteLine();
            Console.WriteLine("You studied:" = hoursStudied);
            Console.ReadLine();
            Console.WriteLine("And Instructor will contact you shortly, Thank you for your answers!");
            Console.ReadLine();
        }   
