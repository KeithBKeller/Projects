using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Iteration
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Please write your name:");
            string yourName = Console.ReadLine();

            string[] namesList = { "Bucky", "Jarvis", "Keith", "Peter", "Steve", "Tony", yourName };

            for (int n = 0; n < namesList.Length; n++)
            {
                Console.WriteLine(namesList[n]);
            }
            Console.ReadLine();
        }
    }
}
